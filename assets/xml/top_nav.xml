<?xml version="1.0" encoding="utf-8" ?>
<data>	

	<include id="_globals"/>

	<sprite id="black" x="0" y="0" width="100%" height="16" color="0xff222034"/>

	<button id="new" label="New" use_def="text_button" x="10" y="0">
		<param type="string" value="new"/>
	</button>

	<button id="load" label="Load" use_def="text_button" x="new.x+30" y="0">
		<param type="string" value="load"/>
	</button>

	<button id="save" label="Save" use_def="text_button" x="load.x+30" y="0">
		<param type="string" value="save"/>
	</button>

	<button id="play" label="Play" use_def="text_button" x="save.x+30" y="0">
		<param type="string" value="play"/>
	</button>

	<button id="quit" label="Quit" use_def="text_button" x="play.x+30" y="0">
		<param type="string" value="quit"/>
	</button>
</data>